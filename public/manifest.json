{
	"testId": "Vision Board",
	"name": "Vision Board",
	"version": "1.2.0",
	"manifestVersion": 2,
	"requirements": {
		"apps": [
			{
				"name": "Express",
				"apiVersion": 1,
				"supportedDeviceClass": ["desktop"]
			}
		]
	},
	"entryPoints": [
		{
			"type": "panel",
			"id": "panel1",
			"main": "index.html",
			"permissions": {
				"sandbox": ["allow-popups", "allow-popups-to-escape-sandbox"]
			}
		}
	]
}
